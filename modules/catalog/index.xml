<?xml version="1.0" encoding="UTF-8"?>
<config>
	<default>
		<category url="krakow-stare-miasto"/>
	</default>
	<dataGridColumnsDefault>
		<name name="Nazwa">
			<row type="Html" class="full-a">
				<content><![CDATA[<a class="none" href='stomatolog/{id}'>{name}</a>]]></content>
			</row>
		</name>
		<adress name="Adress">
			<href>/catalog/index/index</href>
			<row type="Html" class="full-a span-5">
				<content><![CDATA[<a class="none" href='stomatolog/{id}'>{adress}</a>]]></content>
			</row>
		</adress>
		<options class="span-3 last" rowspan="2">
			<name><![CDATA[<input class='search' type='submit' value='Szukaj!' />]]></name>
			<row type="Html">
				<content><![CDATA[<a class="vcard small" href='stomatolog/{id}'>zobacz wizytówkę</a>]]></content>
			</row>
		</options>
	</dataGridColumnsDefault>

	<dataGridColumns extends="dataGridColumnsDefault">
		<name type="Order" filter="Text">
			<href>/catalog/index/index</href>
		</name>
		<adress type="Order" filter="Text">
			<href>/catalog/index/index</href>
		</adress>
	</dataGridColumns>

	<dataGridColumnsAZ extends="dataGridColumnsDefault">
		<options name="" />
	</dataGridColumnsAZ>
</config>